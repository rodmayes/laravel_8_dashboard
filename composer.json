{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "alkhachatryan/laravel-web-console": "^3.3",
        "asantibanez/laravel-blade-sortable": "^1.3",
        "bugsnag/bugsnag-laravel": "^2.21",
        "doctrine/dbal": "^3.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "glebred/search-multiselect-input": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "lab404/laravel-impersonate": "^1.7",
        "laravel/dusk": "^6.11",
        "laravel/framework": "^8.12",
        "laravel/sanctum": "^2.8",
        "laravel/slack-notification-channel": "^2.5",
        "laravel/telescope": "^4.10",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.2",
        "livewire/livewire": "^2.3",
        "nesbot/carbon": "^2.69",
        "opcodesio/log-viewer": "^3.1",
        "power-components/livewire-powergrid": "^2.4",
        "rap2hpoutre/laravel-log-viewer": "^2.3",
        "rappasoft/laravel-livewire-tables": "^2.15",
        "robersonfaria/laravel-database-schedule": "^1.3",
        "spatie/laravel-activitylog": "^3.17",
        "studio/laravel-totem": "^10.0",
        "vxm/laravel-async": "^2.2",
        "wireui/wireui": "^1.17"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.19",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
